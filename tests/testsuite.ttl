@prefix : <https://cogito.iot.linkeddata.es/tests/building#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc:    <http://purl.org/dc/terms/> .
@prefix dcterms: <http://purl.org/dc/terms/> .


:TestsuiteBuilding rdf:type <http://w3id.org/def/vtc#TestSuite>, owl:NamedIndividual;
            dcterms:source "The COGITO Building ontology".

:Test1 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        dc:description "A building element is related to a property" ;
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;
       <https://w3id.org/def/vtc#desiredBehaviour> "Element relatesToProperty core:Property" .

:Test2 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
		dc:description "A building element is a spatial thing" ;
		<https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;
       <https://w3id.org/def/vtc#desiredBehaviour> "Element subClassOf SpatialThing" .

:Test3 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
       dc:description "A building has a space";
       <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;
       <https://w3id.org/def/vtc#desiredBehaviour> "Building hasSpace Space" .

:Test4 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
       dc:description "A building contains a zone"  ;       
       <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;	
       <https://w3id.org/def/vtc#desiredBehaviour> "Building containsZone Zone" .

:Test5 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        dc:description "A component has quality information"      ;    
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;
       <https://w3id.org/def/vtc#desiredBehaviour> "Element hasQualityInformation QualityInformation" .

:Test6 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        dc:description "A zone has a space"      ;
       <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;
       <https://w3id.org/def/vtc#desiredBehaviour> "Zone hasSpace Space" .

:Test7 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
       dc:description "Each element in a building is a spatial thing"  ;      
       dc:description "Each element has a location";
       <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;         
       <https://w3id.org/def/vtc#desiredBehaviour> "Element subclassOf SpatialThing" .

:Test8 rdf:type <https://w3id.org/def/vtc#TestCaseDesign> ,
                owl:NamedIndividual ;
        dc:description "A building is composed by elements"   ;     
        <https://w3id.org/def/vtc#belongsTo> :TestsuiteBuilding;
       <https://w3id.org/def/vtc#desiredBehaviour> "Building hasElement Element" .